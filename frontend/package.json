		"svelte-check": "^4.3.3",
		"tailwindcss": "^3.4.18",
		"typescript": "^5.5.2",
		"typescript-eslint": "^8.46.1",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"@codemirror/lang-javascript": "^6.2.4",
		"@codemirror/state": "^6.5.2",
		"@codemirror/view": "^6.38.6",
		"codemirror": "^6.0.2",
		"svelte-i18n": "^4.0.1",
		"svelte-sonner": "^1.0.5"
	}
